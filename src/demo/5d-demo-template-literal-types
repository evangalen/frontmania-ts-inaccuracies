const _1 = document.querySelector('div');
//    ^?

const _2 = document.querySelector('div.a-class');
//    ^?

// âœ‚â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´â•´
// ðŸ º only exact 'div' string literal returns a HTMLDivElement
(() => {
  const _1 = document.querySelector('div');
  //    ^?

  const _2 = document.querySelector('div.a-class');
  //    ^?
})(); // ðŸ ¼
// ðŸ º side-effect import: `import type from "typed-query-selector";`
(() => {
  // @ts-expect-error TS1232: An import declaration can only be used at the top level of a namespace or module.
  import type from 'typed-query-selector';

  const _1 = document.querySelector('div');
  //    ^?

  const _2 = document.querySelector('div.a-class');
  //    ^?
})(); // ðŸ ¼
